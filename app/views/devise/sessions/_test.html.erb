<h2>かんたんログイン</h2>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
 <div class="field">
 <%= f.hidden_field :email, value: "test@example.com"%>
 </div>

 <div class="field">
 <%= f.hidden_field :password, value: "password"%>
 </div>

 <div class="actions">
 <%= f.submit "かんたんログイン" %>
 </div>
<% end %>