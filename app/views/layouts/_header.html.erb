<header class="navbar navbar-fixed-top navbar-inverse">
    <div class="container">
        <%= link_to "LET'S VolleyBall", '#', id: "logo" %>
            <nav>
                <% if user_signed_in? %>
                    <%= link_to 'アカウント情報の変更', edit_user_registration_path %>
                    <%= link_to 'プレイヤー情報登録', new_player_path%>   
                    <%= link_to 'ログアウト', destroy_user_session_path, method: :delete %>
                    <% unless  @player.nil? %>
                        <%= link_to 'プレイヤーマイページ', controller: "players" ,method: :get %>
                    <% end %>
                <% else %>
                    <%= link_to '今すぐ登録', new_user_registration_path %>
                    <%= link_to 'ログイン', new_user_session_path %>
                <% end %>
            </nav>
    </div>
</header>